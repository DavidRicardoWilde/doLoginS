<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="model">
	<class name="model.BaseFeatureBean" table="base_feature">
		<id name="fid" column="fid">
			<generator class="native"></generator>
		</id>
		<property name="pid"></property>
		<property name="id"></property>
		<property name="fname"></property>
		<property name="furl"></property>
		<property name="ficon"></property>
		<property name="pwinfo"></property>
		<property name="isroot"></property>
		<property name="ordid"></property>
		<property name="fmemo"></property>
	</class>
</hibernate-mapping>